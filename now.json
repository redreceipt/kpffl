{
  "alias": ["kpffl.com"],
  "routes": [{ "src": "/.*", "dest": "/index.py" }],
  "builds": [{ "src": "*.py", "use": "@now/python" }],
  "env": {
    "GOOGLE_MEET_URI": "@google-meet-uri",
    "DISCORD_URI": "@discord-uri",
    "SECRET_KEY": "@secret-key",
    "AUTH0_SECRET": "@auth0-secret",
    "SPORTSDATA_KEY": "@sportsdata-key",
    "LEAGUE_ID": "@league-id"
  },
  "regions": ["iad", "cle"]
}
